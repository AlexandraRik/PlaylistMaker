<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MainMaterialButton"  parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/text</item>
        <item name="iconTint">@color/black</item>
        <item name="cornerRadius">@dimen/corner_radius</item>
        <item name="iconPadding">8dp</item>
        <item name="iconGravity">textStart</item>
        <item name="android:fontFamily">@font/ys_text_medium</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:elevation">0dp</item>




    </style>


    <style name="SettingTextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/small_text_size</item>
        <item name="android:fontFamily">@font/ys_text_regular</item>

    </style>
    <style name="PageMock">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>




    </style>
    <style name="LinearLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingTop">@dimen/top_padding</item>
        <item name="android:background">@color/blue</item>

    </style>

    <style name="LinearLayoutStyleSetting">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingTop">@dimen/top_padding</item>
        <item name="android:background">@color/white</item>

    </style>


    <style name="SettingTextColor">
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="BackButtonColor">
        <item name="android:src">@drawable/back</item>
    </style>


    <style name="TextViewIcon">
        <item name="android:drawableLeft">@drawable/search_2</item>
        <item name="android:background"> @drawable/rounded_edittext</item>
        <item name="android:textColorHint">@color/gray</item>
    </style>






</resources>


